#:import NoTransition kivy.uix.screenmanager.NoTransition

<MyScreenManager>:
    transition: NoTransition()
    canvas:
        Color:
            rgba: 1,1,1,1
        Rectangle:
            size: self.size
    Splash:
        name: "splash"

<Splash>:
    BoxLayout:
        orientation:"vertical"
        canvas:
            Ellipse:
                size: self.size*0.2    
        
    